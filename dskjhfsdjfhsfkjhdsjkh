-- Scripts:

local function NAPQEL_fake_script() -- tab1.LocalScript 
	local script = Instance.new('LocalScript', tab1)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.killframe.Visible = true
		script.Parent.Parent.gamingframe.Visible = false
		script.Parent.Parent.creditsframe.Visible = false
		script.Parent.Parent.settingsframe.Visible = false
		script.Parent.Parent.lpframe.Visible = false
	end)
end
coroutine.wrap(NAPQEL_fake_script)()
local function CKDEQ_fake_script() -- tab2.LocalScript 
	local script = Instance.new('LocalScript', tab2)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.killframe.Visible = false
		script.Parent.Parent.gamingframe.Visible = false
		script.Parent.Parent.creditsframe.Visible = false
		script.Parent.Parent.settingsframe.Visible = false
		script.Parent.Parent.lpframe.Visible = true
	end)
end
