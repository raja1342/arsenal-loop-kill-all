fly.Name = "fly (PC)"
fly.Parent = lpframe
fly.BackgroundColor3 = Color3.fromRGB(121, 121, 121)
fly.BorderColor3 = Color3.fromRGB(145, 145, 145)
fly.BorderSizePixel = 0
fly.Position = UDim2.new(0.0670975521, 0, 0.374046177, 0)
fly.Size = UDim2.new(0, 108, 0, 52)
fly.Font = Enum.Font.Gotham
fly.Text = "Fly"
fly.TextColor3 = Color3.fromRGB(0, 0, 0)
fly.TextSize = 29.000
fly.TextWrapped = true
fly.MouseButton1Down:Connect(function()


	local Players = game:GetService("Players")
	local LocalPlayer = Players.LocalPlayer


	repeat wait() until LocalPlayer and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Torso") and LocalPlayer.Character:FindFirstChild("Humanoid")

	local mouse = LocalPlayer:GetMouse()
	local torso = LocalPlayer.Character.Torso
	local flying = true
	local ctrl = {f = 0, b = 0, l = 0, r = 0}
	local maxspeed = 50
	local speed = 0


	local function Fly()
		local bg = Instance.new("BodyGyro", torso)
		bg.P = 9e4
		bg.MaxTorque = Vector3.new(9e9, 9e9, 9e9)
		bg.CFrame = torso.CFrame
		local bv = Instance.new("BodyVelocity", torso)
		bv.Velocity = Vector3.new(0, 0.1, 0)
		bv.MaxForce = Vector3.new(9e9, 9e9, 9e9)

		repeat wait()
			LocalPlayer.Character.Humanoid.PlatformStand = true

			if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then
				speed = speed + .5 + (speed / maxspeed)
				if speed > maxspeed then speed = maxspeed end
			else
				speed = speed - 1
				if speed < 0 then speed = 0 end
			end

			if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then
				bv.Velocity = ((workspace.CurrentCamera.CFrame.LookVector * (ctrl.f + ctrl.b)) + ((workspace.CurrentCamera.CFrame * CFrame.new(ctrl.l + ctrl.r, (ctrl.f + ctrl.b) * .2, 0).p) - workspace.CurrentCamera.CFrame.p)) * speed
			else
				bv.Velocity = Vector3.new(0, 0.1, 0)
			end

			bg.CFrame = workspace.CurrentCamera.CFrame * CFrame.Angles(-math.rad((ctrl.f + ctrl.b) * 50 * speed / maxspeed), 0, 0)
		until not flying

		ctrl = {f = 0, b = 0, l = 0, r = 0}
		speed = 0
		bg:Destroy()
		bv:Destroy()
		LocalPlayer.Character.Humanoid.PlatformStand = false
	end


	mouse.KeyDown:connect(function(key)
		if key:lower() == "w" then
			ctrl.f = 1
		elseif key:lower() == "s" then
			ctrl.b = -1
		elseif key:lower() == "a" then
			ctrl.l = -1
		elseif key:lower() == "d" then
			ctrl.r = 1
		end
	end)

	mouse.KeyUp:connect(function(key)
		if key:lower() == "w" then
			ctrl.f = 0
		elseif key:lower() == "s" then
			ctrl.b = 0
		elseif key:lower() == "a" then
			ctrl.l = 0
		elseif key:lower() == "d" then
			ctrl.r = 0
		end
	end)

	Fly()  

end)

infjump.Name = "infjump"
infjump.Parent = lpframe
infjump.BackgroundColor3 = Color3.fromRGB(121, 121, 121)
infjump.BorderColor3 = Color3.fromRGB(145, 145, 145)
infjump.BorderSizePixel = 0
infjump.Position = UDim2.new(0.384992301, 0, 0.366078049, 0)
infjump.Size = UDim2.new(0, 108, 0, 52)
infjump.Font = Enum.Font.Gotham
infjump.Text = "Infinite Jump"
infjump.TextColor3 = Color3.fromRGB(0, 0, 0)
infjump.TextScaled = true
infjump.TextSize = 1.000
infjump.TextWrapped = true
infjump.MouseButton1Down:Connect(function()
	local Player = game.Players.LocalPlayer
	local UserInputService = game:GetService("UserInputService")

	local jumpConnection


	local function enableJump()
		jumpConnection = UserInputService.JumpRequest:Connect(function()
			if Player.Character and Player.Character:FindFirstChildOfClass("Humanoid") then
				Player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
			end
		end)
	end


	enableJump()

	
	Player.CharacterAdded:Connect(function()
		if jumpConnection then
			jumpConnection:Disconnect()
		end
	end)

end)

noclip.Name = "noclip"
noclip.Parent = lpframe
noclip.BackgroundColor3 = Color3.fromRGB(121, 121, 121)
noclip.BorderColor3 = Color3.fromRGB(145, 145, 145)
noclip.BorderSizePixel = 0
noclip.Position = UDim2.new(0.70709753, 0, 0.366078049, 0)
noclip.Size = UDim2.new(0, 108, 0, 52)
noclip.Font = Enum.Font.Gotham
noclip.Text = "Noclip"
noclip.TextColor3 = Color3.fromRGB(0, 0, 0)
noclip.TextScaled = true
noclip.TextSize = 1.000
noclip.TextWrapped = true
noclip.MouseButton1Down:Connect(function()
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local noclipConnection
	
	local function enableNoclip()
		noclipConnection = game:GetService("RunService").Stepped:Connect(function()
			for _, part in pairs(character:GetChildren()) do
				if part:IsA("BasePart") then
					part.CanCollide = false
				end
			end
		end)
	end
	
	enableNoclip()
	
	player.CharacterAdded:Connect(function()
		if noclipConnection then
			noclipConnection:Disconnect()
		end
		character = player.Character
	end)
	

end)

