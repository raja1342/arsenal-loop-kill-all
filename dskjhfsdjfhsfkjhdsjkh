antie.MouseButton1Click:Connect(function()
	local workspace = game:GetService("Workspace")
	local c4ModelName = "C4 Model"
	local grenadeModelName = "Grenade Model"
	local function teleportModelChildren(modelName)
		local model = workspace:FindFirstChild(modelName)
		if model and model:IsA("Model") then
			for _, child in ipairs(model:GetChildren()) do
				if child:IsA("BasePart") then
					child.Position = Vector3.new(-99, -50, 198)
				end
			end
		end
	end
	while true do
		teleportModelChildren(c4ModelName)
		teleportModelChildren(grenadeModelName)
		wait(0.1)
	end

end)

