local targetUsers = { "ii_dino122", "sinstick2", "Wattesigma739", "Termux_404" }
local players = game:GetService("Players")

local function checkUser(player)
    for _, name in ipairs(targetUsers) do
        if player.Name == name then
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Owner/Admin alert!",
                Text = name .. " is in the game.",
                Duration = 5
            })
            break
        end
    end
end

players.PlayerAdded:Connect(checkUser)

for _, player in ipairs(players:GetPlayers()) do
    checkUser(player)
end


wait(0.1)

function Bypass()
    local Script = game.Players.LocalPlayer.Character:WaitForChild("LocalScript")
    if Script then
        Script:Destroy()
    end
end

game.Players.LocalPlayer.CharacterAdded:Connect(function()
    Bypass()
end)

Bypass()


