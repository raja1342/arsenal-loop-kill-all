--kill aura nigger shot oh mee godsdklfsdjlkfjs
shared.settings = {
	killaura = false,
	targets = {}, 
}

local function getchar(plr, yield)
	local plr = plr or lp
	return plr.Character or yield and plr.CharacterAdded:Wait()
end

local function gethumanoid(plr, yield)
	local char = getchar(plr, yield)
	return yield and char:WaitForChild("Humanoid") or char:FindFirstChildWhichIsA("Humanoid")
end

local function getDamageRemote()
	local char = getchar(nil, true)
	local Tool = char:FindFirstChild("Pencil")
	return Tool and Tool:FindFirstChild("DamageRemote")
end

local function fucktargets()
	local targets = {}
	for _, v in ipairs(Players:GetPlayers()) do
		if v ~= lp then
			local char = getchar(v)
			if char and not char:FindFirstChildWhichIsA("ForceField") then
				local Humanoid = gethumanoid(v)
				if Humanoid and Humanoid.Health > 0 then
					table.insert(targets, v)  
				end
			end
		end
	end
	return targets
end

local function KillAuraLoop()
	if not shared.settings.killaura then return end 

	local DamageRemote = getDamageRemote()
	if DamageRemote then
		local targets = fucktargets()
		if #targets > 0 then
			for _, target in ipairs(targets) do
				for i = 1, 5 do
					if not shared.settings.killaura then return end 

					local character = lp.Character
					if character and character:FindFirstChild("HumanoidRootPart") then
						local targetHRP = target.Character and target.Character:FindFirstChild("HumanoidRootPart")
						if targetHRP then
							character:MoveTo(targetHRP.Position - (targetHRP.CFrame.LookVector * 1))
							DamageRemote:FireServer(gethumanoid(target))
						end
					end
					wait(1)
				end
			end
		end
	else
		local Tool = lp.Character:FindFirstChild("Pencil")
		if not Tool and not shared.toolNotified then
		

			shared.toolNotified = true  
			StopKillAura()  
			shared.settings.killaura = false  
			aura.Text = "Toggle KillAura"  
		end
	end
end

local function onCharacterAdded(character)
	local humanoid = character:WaitForChild("Humanoid")
	humanoid.Died:Connect(function()
		if shared.settings.killaura then
			local toolListener
			toolListener = character.ChildAdded:Connect(function(child)
				if child.Name == "Pencil" then
					shared.settings.killaura = true
					KillAuraLoop()
					toolListener:Disconnect()
				end
			end)
		end
	end)
end

lp.CharacterAdded:Connect(onCharacterAdded)

if lp.Character then
	onCharacterAdded(lp.Character)
end


local function StartKillAura()
	if not shared.activeScript then  
		shared.activeScript = RunService.Heartbeat:Connect(KillAuraLoop)
		shared.toolNotified = false  
	end
end

local function StopKillAura()
	if shared.activeScript then
		shared.activeScript:Disconnect()
		shared.activeScript = nil
		shared.settings.targets = {}  
		shared.toolNotified = false 
	end
end

aura.MouseButton1Click:Connect(function()
	shared.settings.killaura = not shared.settings.killaura
	aura.Text = shared.settings.killaura and "Kill All On" or "Kill All Off"

	if shared.settings.killaura then

		shared.settings.targets = fucktargets()
		StartKillAura()  
	else
		StopKillAura()  
	end
end)

lp.CharacterAdded:Connect(function()
	if shared.settings.killaura then
		StartKillAura()
	end
end)

if shared.settings.killaura then
	StartKillAura()
end



