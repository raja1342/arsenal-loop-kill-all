local function LHTJ_fake_script() -- wsr.LocalScript 
	local script = Instance.new('LocalScript', wsr)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")

	local textBox = script.Parent 
	local minWalkSpeed = 16 
	local maxWalkSpeed = 1000

	textBox.FocusLost:Connect(function(enterPressed)
		if enterPressed then
			local inputSpeed = tonumber(textBox.Text) 

			if inputSpeed then
				
				inputSpeed = math.clamp(inputSpeed, minWalkSpeed, maxWalkSpeed)
				humanoid.WalkSpeed = inputSpeed 
				textBox.Text = tostring(inputSpeed) 
			else
				
				humanoid.WalkSpeed = minWalkSpeed
				textBox.Text = tostring(minWalkSpeed)
			end
		end
	end)

end
coroutine.wrap(LHTJ_fake_script)()
