--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local StarterGui = game:GetService("StarterGui")
local lp = Players.LocalPlayer

shared.settings = {
    killaura = false,
    targets = {},
}

local targetNotified = {}
local pencilskibidi = false

local function getchar(plr, yield)
    local plr = plr or lp
    return plr.Character or yield and plr.CharacterAdded:Wait()
end

local function gethumanoid(plr, yield)
    local char = getchar(plr, yield)
    return yield and char:WaitForChild("Humanoid") or char:FindFirstChildWhichIsA("Humanoid")
end

local function getDamageRemote()
    local char = getchar(nil, true)
    local Tool = char:FindFirstChild("Pencil")
    return Tool and Tool:FindFirstChild("DamageRemote")
end

local function fucktargets()
    local targets = {}
    for _, targetName in ipairs(shared.settings.targets) do
        for _, v in ipairs(Players:GetPlayers()) do
            if (v.Name:lower() == targetName:lower() or v.DisplayName:lower() == targetName:lower()) and v ~= lp then
                local char = getchar(v)
                if char and not char:FindFirstChildWhichIsA("ForceField") then
                    local Humanoid = gethumanoid(v)
                    if Humanoid and Humanoid.Health > 0 then
                        table.insert(targets, v)
                    end
                end
            end
        end
    end
    return targets
end

local function sendNotification(title, text, duration)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration,
    })
end

killon.MouseButton1Click:Connect(function()
    shared.settings.killaura = not shared.settings.killaura
    killon.Text = shared.settings.killaura and "Loopkill On" or "Loopkill Off"

    if not shared.settings.killaura then
        targetNotified = {}
        pencilskibidi = false
    end
end)

users.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        shared.settings.targets = {}
        for targetName in users.Text:gmatch("[^,]+") do
            table.insert(shared.settings.targets, targetName:match("^%s*(.-)%s*$"))
        end
    end
end)

clear.MouseButton1Click:Connect(function()
    shared.settings.targets = {}
    users.Text = ""
end)

local function onCharacterAdded()
    while not lp.Character do
        wait()
    end
    if shared.settings.killaura then
        
    end
end

lp.CharacterAdded:Connect(onCharacterAdded)

local function onToolEquipped()
    if shared.settings.killaura then
        killon.Text = "Loopkill On"
    else
        killon.Text = "Loopkill Off"
    end
end

lp.CharacterAdded:Connect(function()
    local char = lp.Character or lp.CharacterAdded:Wait()
    local tool = char:WaitForChild("Pencil", 5)
    if tool then
        tool.Equipped:Connect(onToolEquipped)
    end
end)

shared.activeScript = RunService.Heartbeat:Connect(function()
    if shared.settings.killaura then
        local DamageRemote = getDamageRemote()
        local targets = fucktargets()

        local Tool = lp.Character and lp.Character:FindFirstChild("Pencil")
        if not Tool and not pencilskibidi then
            sendNotification("Tool is NOT equipped", "Please equip the Pencil for this script to work!", 5)
            pencilskibidi = true
            shared.settings.killaura = false
            killon.Text = "Loopkill On"
            return
        elseif Tool then
            pencilskibidi = false
        end

        if DamageRemote and #targets > 0 then
            for _, target in ipairs(targets) do
                local character = lp.Character
                local targetChar = target.Character
                if targetChar then
                    if not targetNotified[target] then
                        target.AncestryChanged:Connect(function(_, parent)
                            if not parent and shared.settings.killaura then
                                targetNotified[target] = true
                            end
                        end)
                    end

                    if character and character:FindFirstChild("HumanoidRootPart") and targetChar:FindFirstChild("HumanoidRootPart") then
                        character.HumanoidRootPart.CFrame = targetChar.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0)
                        DamageRemote:FireServer(gethumanoid(target))

                        wait(1) 

                        break
                    end
                end
            end
        end
    end
end)





Frame.Parent = ScreenGui
