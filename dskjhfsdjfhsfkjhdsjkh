antis.MouseButton1Click:Connect(function()
	local workspace = game:GetService("Workspace")
	local spikeTrapModelName = "Spike Trap"

	local function disableSpikeTraps()
		for _, model in ipairs(workspace:GetChildren()) do
			if model:IsA("Model") and model.Name == spikeTrapModelName then
				local button = model:FindFirstChild("Button")
				if button then
					button.CanTouch = false
				end
			end
		end
	end

	while true do
		disableSpikeTraps()
		wait(0.1) 
	end
end)



