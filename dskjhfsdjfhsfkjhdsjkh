	local player = game.Players.LocalPlayer

local function nigga2()
    local screenGui = player:WaitForChild("PlayerGui"):WaitForChild("Menu Screen")

    local function hideChildren()
        for _, child in pairs(screenGui:GetChildren()) do
            if child:IsA("GuiObject") then
                child.Visible = false
            end
        end
    end

    while true do
        hideChildren()
        wait(0.1)
    end
end

